{
  "$schema": "https://raw.githubusercontent.com/nicedoc/tauri/master/crates/tauri-utils/schema.json",
  "productName": "One Good Hour",
  "version": "0.5.1",
  "identifier": "com.onegoodhour.app",
  "build": {
    "frontendDist": "frontend"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "One Good Hour",
        "width": 600,
        "height": 640,
        "minWidth": 600,
        "minHeight": 300
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
